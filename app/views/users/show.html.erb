
<p> <%= link_to "增加新项目", new_project_path %> </p>

<% if @projects.blank? %>
  no  projects
<% else %>
  <p>你自己的项目:</p>
  <% for project in @projects %>
    <ul>
      <li> <%= link_to project.name, project_path(project.id) %>   <%= project.description %> </li>
    </ul>
  <% end %>
<% end %>

<p>你参与的项目:</p>

